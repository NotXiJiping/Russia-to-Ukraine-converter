{
	"manifest_version": 2,

	"name": "Russia to Ukraine converter",
    "short_name": "Snake People",
	"description": "Replaces the text 'Russia' with 'North-Ukraine'.",
	"author": "Eric Bailey",
	"version": "2.3",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
